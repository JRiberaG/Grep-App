    <Container class="javax.swing.JDialog" name="dialogHowTo">
      <Properties>
        <Property name="title" type="java.lang.String" value="How to use the app - GrepApp"/>
        <Property name="minimumSize" type="java.awt.Dimension" editor="org.netbeans.beaninfo.editors.DimensionEditor">
